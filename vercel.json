{
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python",
      "config": {
        "pipArgs": [
          "--break-system-packages"
        ]
      }
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}